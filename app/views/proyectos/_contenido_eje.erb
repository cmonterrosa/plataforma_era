<script type="text/javascript">
  <%#*var lineas_accion_id = new LiveValidation( 'eje_lineas_accion_id' );%>
  <%#*lineas_accion_id.add( Validate.Presence );%>
  <%#*lineas_accion_id.add( Validate.Exclusion, { within: [ '' , ' ' ] } );%>

  <%#*var indicadore_id = new LiveValidation( 'eje_indicadore_id' );%>
