          <tr>
            <td colspan="2" align="right" ><b>TOTALES: &nbsp; </b></td>
            <td align="right"><b><%= @e_maxptos %></b></td>
            <td align="right"><b><%= @e_totalptos %></b></td>
            <td colspan="4"></td>
          </tr>
          <% end %>
        </table>
        <% end %>
      </div>

      <div class="grid-12-12 clear">
