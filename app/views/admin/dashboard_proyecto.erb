<%=render :partial => "flash_messages"%>

<div class="container">
  <div class="formee">
    <table align="center" class="subprincipal" width="95%" cellspacing="5" style="BORDER-RIGHT: #cccccc 1px solid; BORDER-TOP: #cccccc 1px solid; BORDER-LEFT: #cccccc 1px solid; BORDER-BOTTOM: #cccccc 1px solid;">
      <tr class="tdiagnostico">
        <td align="center" ><label style=" font-weight: bold; " >MONITOR DE EVALUACIÓN DE EVIDENCIAS DE LOS AVANCES</label></td>
      </tr>

      <tr class="tdiagnostico">
        <td align="center" ><label style=" font-weight: bold; " >AVANCE <%=@avance%></label></td>
      </tr>
      <tr class="tdiagnostico">
        <td align="center" ><label>ESCUELA: &nbsp; &nbsp;<b><%= @escuela.nombre %></b>, &nbsp; CLAVE: &nbsp; &nbsp;<b><%= @escuela.clave %></b>  </label></td>
      </tr>

      <tr class="tdiagnostico">
        <td align="center" ><label>MUNICIPIO: &nbsp; &nbsp;<b><%= @escuela.municipio %></b>, &nbsp; LOCALIDAD: &nbsp; &nbsp;<b><%= @escuela.localidad %></b> &nbsp; NIVEL: &nbsp; &nbsp;<b><%=h (@escuela.nivel)? @escuela.nivel.descripcion : "" %></b> </label></td>
      </tr>

      <tr>
          <td colspan="3">
              <div class="formee" style="text-align:center" >
                  <%avance = (@avance == "1")? 2 : 1%>
                  <fieldset>
                    <h1><%= image_tag 'era/iconos/flecha_inversa.png' %>&nbsp;<%= link_to "   REGRESAR A MENU", {:controller => "admin", :action => "menu_escuela", :id => @user}, :style => "color:#0489B1; text-decoration: none;", :title => "IR A MENU" %> &nbsp; | &nbsp; <%= image_tag 'era/iconos/flecha.png' %>&nbsp;<%= link_to "   IR A AVANCE #{avance}", {:controller => "admin", :action => "dashboard_proyecto", :avance => avance, :id => @user, :proyecto => @proyecto}, :style => "color:#0489B1; text-decoration: none;", :title => "IR A AVANCE #{avance}" %></h1>
                  </fieldset>
              </div>
        </td>
       </tr>

    </table>
    <br />
    <fieldset>

    <!-- EJE 1 -->
    <% if @proyecto.competencia %>
      <div class="grid-12-12 clear">
          <% total_eje1 = 0.0 %>
          <% total_obtenido_eje1 = 0.0 %>
          <% @total_obtenido_proyecto_eje1 = 0 %>
          <% @puntaje_pregunta_diagnostico_eje1 = 0 %>
          <% @puntaje_pregunta_proyecto_eje1 = 0%>
          <table width="98%" border="0" cellspacing="0" cellpadding="0" class="sample" >
            <tr class="tdiagnostico">
              <td colspan="10" align="center" ><label>EJE: 1 &nbsp; <b>DESARROLLO DE COMPETENCIAS</b></label></td>
            </tr>
            <% unless @preguntas_eje1.empty?  %>
              <tr class="d0" style=" font-weight: bold; " >
                <td width="5%" style="vertical-align: middle;" align="center">NP</td>
                <td width="10%" style="vertical-align: middle;"align="center">PUNTAJE PREGUNTA</td>
                <td width="10%" style="vertical-align: middle;"align="center">PUNTAJE DIAGNÓSTICO</td>
                <td width="10%" style="vertical-align: middle;" align="center">PUNTAJE AVANCE</td>
                <!--<td width="10%" style="vertical-align: middle;" align="center">PUNTAJE OBTENIDO</td>-->
                <td width="3%" style="vertical-align: middle;" align="center">&nbsp;</td>
                <td width="32%" style="vertical-align: middle;" align="center">DESCRIPCIÓN EVIDENCIA</td>
                <td width="20%" style="vertical-align: middle;" align="center" colspan="4">ACCIONES</td>
              </tr>
              <%@preguntas_eje1.sort{|p1,p2| p1.numero_pregunta <=> p2.numero_pregunta}.each do |p|%>
                <tr class="d1" style="font-size: 14px;">
                  <% total_eje1 += eval("$competencia_p#{p.numero_pregunta}").to_f * 100 %>
                  <% total_obtenido_eje1 += eval("@competencia_p#{p.numero_pregunta}").to_f %>
                  <%detalle_preguntas_eje1 = Adjunto.find(:all, :conditions => ["proyecto_id = ? and eje_id = ? and numero_pregunta = ? and avance = ?", p.proyecto_id, @eje1.id, p.numero_pregunta, @avance])%>
                  <td align="center" rowspan="<%= detalle_preguntas_eje1.size + 1%>"><%= p.numero_pregunta %></td>
                  <td align="center" style="font-size: 14px;" rowspan="<%=h detalle_preguntas_eje1.size + 1%>"><%= eval("$competencia_p#{p.numero_pregunta}").to_f * 100 %></td>
                  <% @puntaje_pregunta_diagnostico_eje1 = eval("@p_diagnostico.puntaje_eje1_p#{p.numero_pregunta}").to_f %>
                  <% @puntaje_pregunta_proyecto_eje1 = (eval("@competencia_p#{p.numero_pregunta}").to_f > 0.0) ? (eval("@competencia_p#{p.numero_pregunta}").to_f - eval("@p_diagnostico.puntaje_eje1_p#{p.numero_pregunta}").to_f) : 0.0%>
                  <% @total_obtenido_proyecto_eje1 += @puntaje_pregunta_proyecto_eje1 %>
                  <td align="center" style="font-size: 14px;" rowspan="<%=h detalle_preguntas_eje1.size + 1%>"><%= @puntaje_pregunta_diagnostico_eje1 %></td>
                  <td align="center" style="font-size: 14px;" rowspan="<%=h detalle_preguntas_eje1.size + 1%>"><%= @puntaje_pregunta_proyecto_eje1 %></td>
                  <!--<td align="center" style="font-size: 14px;" rowspan="<%=h detalle_preguntas_eje1.size + 1%>"><%= @puntaje_pregunta_diagnostico_eje1 + @puntaje_pregunta_proyecto_eje1 %></td>-->
                </tr>

                <%detalle_preguntas_eje1.each do |eje|%>
                  <tr class="d1">
                  <!-- icono de validado -->
                    <%if eje.validado%>
                      <td style="vertical-align: middle;"><%= image_tag 'admin/disponible.png'%></td>
                    <%else%>
                      <%if eje.validado == false%>
                        <td style="vertical-align: middle;"><%= image_tag 'admin/bloqueado.png'%></td>
                      <%else%>
                        <td style="vertical-align: middle;">-</td>
                      <%end%>
                    <%end%>
                    <td><%= eje.descripcion %></td>
                    <td><%=link_to image_tag('admin/view.png', :size => "30x30", :mouseover => 'admin/view.png') , {:controller => "upload", :action => "download", :id => eje.id}, :class => 'fancybox fancybox.iframe', :title => "Ver Evidencia" %></td>
                    <%if current_user.has_role?("adminplat") || current_user.has_role?("enlaceevaluador")%>
                      <td><%=link_to image_tag('admin/valid.png', :size => "30x30", :mouseover => 'admin/valid.png') , {:controller => "upload", :action => "validar", :id => eje.id, :diagnostico => @diagnostico, :user => @user, :dashboard_proyecto => 1, :avance => @avance }, :html => { :target => "_blank"}, :title => "Evidencia Válida" %></td>
                      <td><%=link_to image_tag('admin/invalid.png', :size => "30x30", :mouseover => 'admin/invalid.png') , {:controller => "upload", :action => "invalidar", :id => eje.id, :diagnostico => @diagnostico, :user => @user, :dashboard_proyecto => 1, :avance => @avance }, :html => { :target => "_blank"}, :title => "Evidencia Inválida" %></td>
                      <td><%=link_to image_tag('admin/delete.png', :size => "30x30", :mouseover => 'admin/delete.png') , {:controller => "upload", :action => "destroy_evidencia", :id => eje.id, :dashboard => 1, :user_id => @user.id, :diagnostico_id => @diagnostico}, :confirm => '¿Esta seguro que desea elimina la evidencia?', :title => "Eliminar Evidencia" %></td>
                    <% else %>
                      <td colspan="3">&nbsp;</td>
                    <% end %>
                  </tr>
                <%end%>
                <tr>
                  <td colspan="10">&nbsp;</td>
                </tr>
              <%end%>
              <tr>
                <td align="right" ><b>TOTALES:</b></td>
                <td align="center"><b><%= @total_puntos_eje1 %></b></td>
                <td></td>
                <td align="center"><b><%= @total_obtenido_proyecto_eje1 %></b></td>
                <td colspan="6"></td>
              </tr>
            <%end%>
          </table>
        </div>
    <% end %>

