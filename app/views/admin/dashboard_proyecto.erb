                <% @puntaje_pregunta_diagnostico_eje2 = eval("@p_diagnostico.puntaje_eje2_p#{p.numero_pregunta}").to_f%>
                <% @puntaje_pregunta_proyecto_eje2 = (eval("@entorno_p#{p.numero_pregunta}").to_f > 0.0) ? (eval("@entorno_p#{p.numero_pregunta}").to_f - eval("@p_diagnostico.puntaje_eje2_p#{p.numero_pregunta}").to_f) : 0.0 %>
